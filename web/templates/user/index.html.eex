<div class="jumbotron">
  <h2>These people use our service, you should too.</h2>
</div>

<table class="table-responsive">
  <thead>
    <th>Name</th>
    <th>Email</th>
  </thead>
  <tbody>
    <%= for user <- @all_users do %>
    <tr>
      <td><%= user.name%></td>
      <td><%= user.email%></td>
    </tr>
    <% end %>
  </tbody>

</table>
<div class="row marketing">
</div>
